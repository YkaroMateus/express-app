{
  "name": "express-app",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/main/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
   "test:unit": "jest --testMatch='<rootDir>/src/tests/domain/**/*.spec.ts'",
    "test:integration": "jest --testMatch='<rootDir>/src/tests/infra/**/*.spec.ts'",
    "test:e2e": "jest --testMatch='<rootDir>/src/tests/e2e/**/*.test.ts'",
    "prisma": "npx prisma studio",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mongoose": "^8.9.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "eslint": "^9.18.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^3.1.9",
    "prisma": "^6.2.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
